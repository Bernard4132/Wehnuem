<p id="notice"><%= notice %></p>

<h1>Wehnleads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phonenumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wehnleads.each do |wehnlead| %>
      <tr>
        <td><%= wehnlead.name %></td>
        <td><%= wehnlead.email %></td>
        <td><%= wehnlead.phonenumber %></td>
        <td><%= link_to 'Show', wehnlead %></td>
        <td><%= link_to 'Edit', edit_wehnlead_path(wehnlead) %></td>
        <td><%= link_to 'Destroy', wehnlead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wehnlead', new_wehnlead_path %>
